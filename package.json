{
  "name": "create-web-cli",
  "version": "0.0.1",
  "description": "A tool for create web project simple.",
  "main": "dist/index.js",
  "scripts": {
    "debug": "nodemon --no-stdin --ignore lib/ --ignore __temp/ ./src/index.ts create",
    "c": "swc ./src -d dist -D -w --include-dotfiles",
    "test": "node ./dist/index.js create",
    "cpi": "copyfiles src/**/source/** dist -u 1 -V",
    "ct": "cp -R src/plugins/st-react-ts/source/ __temp/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/templates-web/create-web-cli.git"
  },
  "keywords": [
    "web",
    "cli"
  ],
  "author": "iWuzhi <24613585972@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/templates-web/create-web-cli/issues"
  },
  "homepage": "https://github.com/templates-web/create-web-cli#readme",
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.130",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^17.0.8",
    "@types/prettier": "^2.4.3",
    "chalk": "4",
    "chokidar": "^3.5.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "art-template": "^4.13.2",
    "commander": "^8.3.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "prettier": "^2.5.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
